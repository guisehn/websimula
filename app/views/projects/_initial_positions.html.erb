<h2>Posições iniciais dos agentes</h2>

<initial-positions-field :read-only="true" :value="<%= @project.initial_positions.to_json %>" :agents="<%= @project.agents.to_json %>"></initial-positions-field>

<% if @project.can_be_edited_by?(current_user) %>
  <br>
  <%= link_to 'Definir posições iniciais', edit_initial_positions_project_path(@project), class: 'btn btn-sm btn-success' %>
<% end %>
